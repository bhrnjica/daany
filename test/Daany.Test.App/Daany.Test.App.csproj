  <ItemGroup>
    <ProjectReference Include="..\..\src\daany.util\daany.util.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="AirPassengers.csv">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
